package Aula02;

public class Caneta {
	private String cor;
	private String modelo;
	private float ponta;
	private boolean tampada;
	private int carga; 
	
	
	void tampar() {
		
	}
	
	void destampar () {
		
	}
	
	void rabiscar() {
		
	}
	
	
	void status () {
		System.out.println("Caneta "+this.cor);
		System.out.println("Modelo: "+this.modelo);
		System.out.println("Carga: "+this.carga);
